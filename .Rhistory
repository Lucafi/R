if (n < 0) 0 else n
}
l1 <- spot - (spread/2)
s1 <- spot
s2 <- spot
l2 <- spot + (spread/2)
data <- data.frame()
for(i in priceAtMaturity-(priceAtMaturitySpread/2):priceAtMaturity+(priceAtMaturitySpread/2)) {
l1r <- removeLoses(i - l1)
s1r <- removeLoses(i - s1)
s2r <- removeLoses(i - s2)
l2r <- removeLoses(i - l2)
row <- c(i,l1r,s1r,s2r,l2r,l1r+s1r+s2r+l2r)
data <- rbind(data, row)
}
headers <- c("Price at Maturity","Long 1", "Short 1", "Short 2", "Long 2", "Total")
names(data) <- headers
data
}
